<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>GZZ_Astro</servlet-name>
        <servlet-class>api.referencial.GZZ_Astro_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GZZ_Astro</servlet-name>
        <url-pattern>/api/gzz_astro/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GZM_Mineral</servlet-name>
        <servlet-class>api.referencial.GZM_Mineral_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GZM_Mineral</servlet-name>
        <url-pattern>/api/gzm_mineral/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>P8M_Centro</servlet-name>
        <servlet-class>api.referencial.P8M_Centro_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>P8M_Centro</servlet-name>
        <url-pattern>/api/p8m_centro/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>R7C_Alimento</servlet-name>
        <servlet-class>api.referencial.R7C_Alimento_Servlet</servlet-class>
