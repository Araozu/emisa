    <servlet>
        <servlet-name>R7C_Empleo</servlet-name>
        <servlet-class>api.referencial.R7C_Empleo_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>R7C_Empleo</servlet-name>
        <url-pattern>/api/r7c_empleo/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RZC_Categoria</servlet-name>
        <servlet-class>api.referencial.RZC_Categoria_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RZC_Categoria</servlet-name>
        <url-pattern>/api/rzc_categoria/</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>